{
  "name": "@adobe/update-prerelease-npm-version",
  "version": "1.0.0",
  "description": "A Github Action to update your node package.json to a prerelease version",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src test",
    "test": "npm run unit-tests",
    "unit-tests": "jest --ci",
    "posttest": "npm run lint",
    "build": "npm run package",
    "package": "ncc build src/index.js -o dist/"
  },
  "keywords": [],
  "author": "Adobe Inc.",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@adobe/eslint-config-aio-lib-config": "^2.0.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.49.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsdoc": "^37.9.7",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.2",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=12"
  }
}
