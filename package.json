{
  "name": "@adobe/update-prerelease-npm-version",
  "version": "1.0.0",
  "description": "A Github Action to update your node package.json to a prerelease version",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src test",
    "test": "npm run unit-tests",
    "unit-tests": "jest --ci",
    "posttest": "npm run lint",
    "build": "npm run package",
    "package": "ncc build src/index.js -o dist/"
  },
  "keywords": [],
  "author": "Adobe Inc.",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.0.1",
    "semver": "^7.5.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-aio-lib-config": "^1.4.0",
    "@vercel/ncc": "^0.33.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsdoc": "^37.9.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.5.1",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=12"
  }
}
